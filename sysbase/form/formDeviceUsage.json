{
  "grid": [
    { "id": "p1", "xs": 12, "class": "main", "table": "devicevisTable", "height": 250 },
    { "id": "p2", "xs": 12, "class": "main", "table": "devicesceneTable", "height": 250 }
  ],
  "spacing": 10,

  "p1": [
    { "prop": "vis_header", "title": "Визуализация", "type": "header", "size": "h6" },
    {
      "title": "$DeviceProperty",
      "type": "table",
      "prop": "devicevisTable",

      "columns": [
        {
          "prop": "container_name",
          "title": "Размещение",
          "type": "link",
          "width": 250,
          "command": "forward",
          "filter": true
        },
        { "prop": "content_str", "title": "Привязки", "type": "text", "width": 350, "filter": true }
      ]
    }
  ],
  "p2": [
    { "prop": "scene_header", "title": "Сценарии", "type": "header", "size": "h6" },
    {
      "title": "$DeviceProperty",
      "type": "table",
      "prop": "devicesceneTable",

      "columns": [
        { "prop": "scene_link", "title": "", "type": "link", "width": 300, "command": "forward", "filter": true },
        { "prop": "realdevs", "title": "Устройства сценария", "type": "text", "width": 150 },
        { "prop": "scene_state", "title": "Состояние", "type": "text", "width": 150 },
        { "prop": "laststartStr", "title": "Время запуска", "type": "text", "width": 150 },
        { "prop": "laststopStr", "title": "Время останова", "type": "text", "width": 150 },
        { "prop": "error", "title": "Ошибка", "type": "text", "width": 250 }
      ]
    }
  ]
}
