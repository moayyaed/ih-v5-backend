{
  "grid": [{ "id": "p1", "xs": 12, "class": "main", "table": "docimagesTreeTable", "height": "fill" }],
  "spacing": 10,

  "p1": [
    {
      "title": "Images",
      "type": "table",
      "prop": "imagesTreeTable",

      "columns": [
        {
          "prop": "image_name",
          "title": "$File",
          "type": "link",
          "command": "forward",
          "width": 300,
          "filter": true
        },
        { "prop": "name", "title": "$Name", "type": "text", "width": 300, "filter": true },

        { "prop": "path", "title": "$Folder", "type": "text", "width": 300, "filter": true },
        { "prop": "error", "title": "", "type": "text", "width": 200 }
      ]
    }
  ]
}
